@import "../../../common/vendor.wxss";

.sb_wrap ._h4 {
  width: 45rpx;
  display: inline-block;
  vertical-align: top;
  font-size: 26rpx;
  color: #999;
  margin-top: 20rpx;
}
.sb_wrap .sb_tip {
  display: inline-block;
  vertical-align: top;
  width: 640rpx;
  margin-top: 20rpx;
}
.sb_wrap .sb_tip ._span {
  font-size: 26rpx;
  color: #999;
  text-align: justify;
  display: block;
  line-height: 35rpx;
  margin-bottom: 10rpx;
}
.imgs_sb {
  margin: 30rpx 0;
  overflow: hidden;
}
.imgs_sb .btn_upload {
  width: 130rpx;
  height: 160rpx;
  float: left;
}
.imgs_sb .btn_upload ._img {
  width: 130rpx;
  height: 160rpx;
}
.imgs_sb ._ul {
  float: left;
  overflow: hidden;
}
.imgs_sb ._ul ._li {
  float: left;
  margin-right: 9rpx;
}
.imgs_sb ._ul ._li ._img {
  width: 130rpx;
  height: 160rpx;
}
.service_icp {
  display: flex;
  justify-content: space-between;
}
.service_icp .imgs_sb2 {
  margin-top: 35rpx;
}
.service_icp .imgs_sb2 ._img {
  width: 335rpx;
  height: 345rpx;
}
.website_wrap .website {
  padding: 20rpx 0;
}
.website_wrap .website ._p {
  display: inline-block;
  vertical-align: top;
  font-size: 26rpx;
  color: #999;
  width: 180rpx;
  line-height: 48rpx;
}
.website_wrap .website ._input,
.website_wrap .website .radio_wrap,
.website_wrap .website ._textarea {
  display: inline-block;
  vertical-align: top;
  width: 490.00000000000006rpx;
  line-height: 48rpx;
  font-size: 26rpx;
  color: #333;
  margin-left: 18rpx;
}
.website_wrap .website ._textarea {
  height: 130rpx;
  font-size: 26rpx;
  color: #999;
  line-height: 1.8;
  padding-top: 10rpx;
}
.website_wrap .website .radio_wrap ._ul {
  overflow: hidden;
}
.website_wrap .website .radio_wrap ._ul ._li {
  width: 150rpx;
  margin-right: 20rpx;
  float: left;
  margin-bottom: 5rpx;
}
.website_wrap .website .radio_wrap ._ul ._li:nth-of-type(3n) {
  margin-right: 0;
}
.website_wrap .website .radio_wrap ._ul ._li.on ._b {
  width: 20rpx;
  height: 20rpx;
  background: url(data:image/png;base64,) no-repeat center;
  background-size: 100%;
}
.website_wrap .website .radio_wrap ._ul ._li ._b {
  display: inline-block;
  width: 20rpx;
  height: 20rpx;
  background: url(data:image/png;base64,) no-repeat center;
  background-size: 100%;
}
.website_wrap .website .radio_wrap ._ul ._li ._span {
  display: inline-block;
  vertical-align: top;
  font-size: 26rpx;
  color: #999;
  margin-left: 15rpx;
}