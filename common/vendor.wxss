
.tab {
  width: 100%;
  height: 100rpx;
  margin-top: 20rpx;
  background: #fff;
}
.tab ._ul {
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.tab ._ul ._li {
  line-height: 100rpx;
  font-size: 28.000000000000004rpx;
  text-align: center;
  color: #333;
  position: relative;
  flex-grow: 1;
}
.tab ._ul ._li.on:after {
  content: "";
  display: block;
  width: 30rpx;
  height: 4rpx;
  background: #60af00;
  position: absolute;
  left: 0;
  right: 0;
  bottom: 0;
  margin: 0 auto;
}
