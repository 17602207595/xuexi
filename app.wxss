.border_4 {
  position: relative;
  box-sizing: border-box;
}
.border_4:after {
  content: "";
  position: absolute;
  top: -50%;
  left: -50%;
  width: 200%;
  height: 200%;
  border: 2rpx solid #e8e8e8;
}
.border_top,
.border_right,
.border_bottom,
.border_left {
  position: relative;
}
.border_top:after,
.border_right:after,
.border_bottom:after,
.border_left:after {
  content: "";
  position: absolute;
}
.border_top:after {
  top: 0;
  left: 0;
  border-top: 2rpx solid #e8e8e8;
  width: 100%;
}
.border_right:after {
  top: 0;
  right: 0;
  border-right: 2rpx solid #e8e8e8;
  height: 100%;
}
.border_bottom:after {
  bottom: 0;
  left: 0;
  border-bottom: 2rpx solid #e8e8e8;
  width: 100%;
}
.border_left:after {
  top: 0;
  left: 0;
  border-left: 2rpx solid #e8e8e8;
  height: 100%;
}
@media (-webkit-min-device-pixel-ratio: 1.5) {
.border_4,
  .border_top,
  .border_right,
  .border_bottom,
  .border_left {
    position: relative;
}
.border_4:after,
  .border_top:after,
  .border_right:after,
  .border_bottom:after,
  .border_left:after {
    transform: scaleY(0.7);
}
}
@media (-webkit-min-device-pixel-ratio: 2) {
.border_4,
  .border_top,
  .border_right,
  .border_bottom,
  .border_left {
    position: relative;
}
.border_4:after,
  .border_top:after,
  .border_right:after,
  .border_bottom:after,
  .border_left:after {
    transform: scaleY(0.5);
}
}
